{
  "info": {
    "name": "Therapy Assistant API",
    "description": "Complete API collection for Therapy Session Assistant",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:5000",
      "type": "string"
    },
    {
      "key": "token",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "url": "{{baseUrl}}/api/auth/register",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"newtherapist@example.com\",\n  \"password\": \"password123\",\n  \"full_name\": \"Dr. New Therapist\"\n}"
            }
          }
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('token', jsonData.access_token);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "url": "{{baseUrl}}/api/auth/login",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"therapist@example.com\",\n  \"password\": \"password123\"\n}"
            }
          }
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/auth/me"
          }
        },
        {
          "name": "Change Password",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/auth/change-password",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"current_password\": \"password123\",\n  \"new_password\": \"newpassword123\"\n}"
            }
          }
        }
      ]
    },
    {
      "name": "Clients",
      "item": [
        {
          "name": "Get All Clients",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/clients"
          }
        },
        {
          "name": "Get Client by ID",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/clients/1"
          }
        },
        {
          "name": "Get Client with Sessions",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": {
              "raw": "{{baseUrl}}/api/clients/1?include_sessions=true",
              "query": [{"key": "include_sessions", "value": "true"}]
            }
          }
        },
        {
          "name": "Create Client",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/clients",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john.doe@email.com\",\n  \"phone\": \"+1-555-0199\",\n  \"notes\": \"Initial consultation scheduled.\"\n}"
            }
          }
        },
        {
          "name": "Update Client",
          "request": {
            "method": "PUT",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/clients/1",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"notes\": \"Updated client notes.\"\n}"
            }
          }
        },
        {
          "name": "Delete Client",
          "request": {
            "method": "DELETE",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/clients/1"
          }
        },
        {
          "name": "Get Client Sessions",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/clients/1/sessions"
          }
        }
      ]
    },
    {
      "name": "Sessions",
      "item": [
        {
          "name": "Get All Sessions",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/sessions"
          }
        },
        {
          "name": "Get Sessions by Client",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": {
              "raw": "{{baseUrl}}/api/sessions?client_id=1",
              "query": [{"key": "client_id", "value": "1"}]
            }
          }
        },
        {
          "name": "Get Session by ID",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": {
              "raw": "{{baseUrl}}/api/sessions/1?include_notes=true&include_insights=true",
              "query": [
                {"key": "include_notes", "value": "true"},
                {"key": "include_insights", "value": "true"}
              ]
            }
          }
        },
        {
          "name": "Create Session",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/sessions",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"client_id\": 1,\n  \"session_date\": \"2025-11-01T14:00:00\",\n  \"duration\": 50,\n  \"status\": \"scheduled\",\n  \"session_type\": \"regular\"\n}"
            }
          }
        },
        {
          "name": "Update Session",
          "request": {
            "method": "PUT",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/sessions/1",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"duration\": 60,\n  \"status\": \"completed\"\n}"
            }
          }
        },
        {
          "name": "Start Session",
          "request": {
            "method": "POST",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/sessions/1/start"
          }
        },
        {
          "name": "Pause Session",
          "request": {
            "method": "POST",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/sessions/1/pause"
          }
        },
        {
          "name": "End Session",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/sessions/1/end",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"duration\": 55\n}"
            }
          }
        },
        {
          "name": "Export Session",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/sessions/1/export"
          }
        },
        {
          "name": "Delete Session",
          "request": {
            "method": "DELETE",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/sessions/1"
          }
        }
      ]
    },
    {
      "name": "Notes",
      "item": [
        {
          "name": "Get All Notes",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/notes"
          }
        },
        {
          "name": "Get Notes by Session",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": {
              "raw": "{{baseUrl}}/api/notes?session_id=1",
              "query": [{"key": "session_id", "value": "1"}]
            }
          }
        },
        {
          "name": "Get Note by ID",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/notes/1"
          }
        },
        {
          "name": "Create Note",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/notes",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": 1,\n  \"content\": \"Client showed significant progress today.\",\n  \"note_type\": \"observation\",\n  \"is_private\": false\n}"
            }
          }
        },
        {
          "name": "Update Note",
          "request": {
            "method": "PUT",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/notes/1",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"content\": \"Updated note content.\"\n}"
            }
          }
        },
        {
          "name": "Delete Note",
          "request": {
            "method": "DELETE",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/notes/1"
          }
        }
      ]
    },
    {
      "name": "AI Insights",
      "item": [
        {
          "name": "Get Insights",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": {
              "raw": "{{baseUrl}}/api/ai/insights?session_id=1",
              "query": [{"key": "session_id", "value": "1"}]
            }
          }
        },
        {
          "name": "Create Insight",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/ai/insights",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": 1,\n  \"insight_type\": \"emotion\",\n  \"label\": \"Emotional State\",\n  \"value\": \"Calm\",\n  \"confidence\": 0.85\n}"
            }
          }
        },
        {
          "name": "Get Live Insights",
          "request": {
            "method": "GET",
            "header": [{"key": "Authorization", "value": "Bearer {{token}}"}],
            "url": "{{baseUrl}}/api/ai/insights/live/1"
          }
        },
        {
          "name": "Analyze Sentiment",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/ai/analyze/sentiment",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"text\": \"I have been feeling much better lately. The techniques we discussed are really helping.\"\n}"
            }
          }
        },
        {
          "name": "Analyze Transcript",
          "request": {
            "method": "POST",
            "header": [
              {"key": "Content-Type", "value": "application/json"},
              {"key": "Authorization", "value": "Bearer {{token}}"}
            ],
            "url": "{{baseUrl}}/api/ai/analyze/transcript",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": 1\n}"
            }
          }
        }
      ]
    },
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "url": "{{baseUrl}}/health"
      }
    }
  ]
}
